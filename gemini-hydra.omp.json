{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Shell }} - {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#4285F4",
          "template": "\u256d\u2500"
        },
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#FFFFFF",
          "background": "#4285F4",
          "leading_diamond": "\ue0b6",
          "template": " {{ if .WSL }}\ue712{{ else }}\ue62a{{ end }} "
        },
        {
          "type": "session",
          "style": "powerline",
          "foreground": "#FFFFFF",
          "background": "#34A853",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .UserName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "#FFFFFF",
          "background": "#FBBC05",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder",
            "max_depth": 3
          },
          "template": " \uf07c {{ .Path }} "
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#0D1117",
          "background": "#EA4335",
          "powerline_symbol": "\ue0b0",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#EA4335{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FBBC05{{ end }}",
            "{{ if gt .Ahead 0 }}#69F0AE{{ end }}",
            "{{ if gt .Behind 0 }}#82B1FF{{ end }}"
          ],
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
        },
        {
          "type": "text",
          "style": "powerline",
          "foreground": "#FFFFFF",
          "background": "transparent",
          "powerline_symbol": "\ue0b0",
          "template": ""
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "node",
          "style": "plain",
          "foreground": "#34A853",
          "template": "\ue718 {{ .Full }} ",
          "properties": {
            "fetch_version": true
          }
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#FBBC05",
          "template": "\ue235 {{ .Full }} ",
          "properties": {
            "fetch_version": true,
            "display_mode": "context"
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#4285F4",
          "template": "\uf073 {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#EA4335",
          "template": "\uf252 {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#4285F4",
          "template": "\u2570\u2500"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#4285F4",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#EA4335{{ end }}"
          ],
          "template": "\u276f "
        }
      ]
    }
  ],
  "secondary_prompt": {
    "foreground": "#4285F4",
    "template": "\u2570\u2500\u276f "
  },
  "transient_prompt": {
    "foreground": "#4285F4",
    "template": "\u276f "
  }
}
